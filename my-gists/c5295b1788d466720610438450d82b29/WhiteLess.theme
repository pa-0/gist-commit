[Control Panel\Desktop]
Wallpaper=%APPDATA%\Microsoft\Windows\Themes\TranscodedWallpaper
PicturePosition=4
;TileWallpaper=0
;WallpaperStyle=10
;Pattern=
;MultimonBackgrounds=0

[Control Panel\Desktop\WindowMetrics]
BorderWidth=0
CaptionHeight=-224
CaptionWidth=-224
IconTitleWrap=1
MenuHeight=-224
MenuWidth=-224
ScrollHeight=-208
ScrollWidth=-208
SmCaptionHeight=-224
SmCaptionWidth=-224
PaddedBorderWidth=0
MinAnimate=0

[VisualStyles]
Path=%SystemRoot%\resources\Themes\Aero\AeroLite.msstyles
ColorStyle=NormalColor
Size=NormalSize
HighContrast=4
ColorizationColor=0XFF4C4A48
AutoColorization=0
Transparency=0
AppMode=Light
SystemMode=Light
VisualStyleVersion=10

[boot]
SCRNSAVE.EXE=

[MasterThemeSelector]
MTSM=RJSPBS

[Sounds]
; IDS_SCHEME_NO_SOUNDS
SchemeName=@%SystemRoot%\System32\mmres.dll,-801
