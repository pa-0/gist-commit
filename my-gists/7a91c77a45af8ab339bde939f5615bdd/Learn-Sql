/*  to see databases in workbench   */
SHOW DATABASES;  

/* to create db - name */
CREATE DATABASE test_db;

/* Working with a Database  */
USE <db_name>;

/*  show active database */
select database();

/*  deleting a database*/
DROP DATABASE <db_name>;

/* to create a table in active db */
CREATE TABLE customers(
id INT,
name VARCHAR(100)
);

/* show schema of a table   DESC table;*/
DESC customers;

